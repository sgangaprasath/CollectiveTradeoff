Cell[BoxData[{
 RowBox[{
  RowBox[{"vintND", "[", 
   RowBox[{"\[Phi]st_", ",", "k_", ",", " ", "s_", ",", " ", "sc_"}], "]"}], ":=", 
  RowBox[{
   FractionBox["\[Phi]st", "k"], 
