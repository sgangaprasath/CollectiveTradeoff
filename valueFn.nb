Cell[BoxData[{
 RowBox[{
  RowBox[{"vint", "[", 
   RowBox[{"\[Phi]st_", ",", "ko_", ",", " ", "s_", ",", " ", "sc_"}], "]"}], 
  ":=", 
  RowBox[{
   FractionBox["\[Phi]st", "ko"], 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"Exp", "[", 
      RowBox[{
       RowBox[{"-", "ko"}], " ", 
       RowBox[{"s", "/", "\[Phi]st"}]}], "]"}], "-", 
     RowBox[{"Exp", "[", 
      RowBox[{
       RowBox[{"-", "ko"}], " ", 
       RowBox[{"sc", "/", "\[Phi]st"}]}], "]"}]}], 
    ")"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"\[Psi]", "[", 
    RowBox[{"s_", ",", "sc_", ",", "vo_", ",", "d_", ",", "\[Nu]_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{
     FractionBox[
      RowBox[{
       SuperscriptBox["vo", "2"], "d"}], "\[Nu]"], 
     RowBox[{"(", 
      RowBox[{
       FractionBox[
        RowBox[{"2", " ", "\[Nu]", " ", 
         RowBox[{"(", 
          RowBox[{"s", "-", "sc"}], ")"}]}], "vo"], " ", "-", " ", 
       RowBox[{"Exp", "[", 
        RowBox[{
         RowBox[{"-", "2"}], "\[Nu]", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"s", "-", "sc"}], ")"}], "/", "vo"}]}], "]"}], "+", 
       RowBox[{"4", 
        RowBox[{"Exp", "[", 
         RowBox[{
          RowBox[{"-", "\[Nu]"}], 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"s", "-", "sc"}], ")"}], "/", "vo"}]}], "]"}]}], "-", 
       "3"}], ")"}]}], "+", "0.1"}]}], "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"vOU", "[", 
     RowBox[{
     "s_", ",", "sc_", ",", "vo_", ",", "d_", ",", "\[Nu]_", ",", " ", 
      "\[Sigma]_", ",", " ", "\[Mu]_"}], "]"}], ":=", " ", 
    FractionBox[
     RowBox[{"2", " ", "\[Sigma]", " ", "\[Mu]"}], 
     RowBox[{"\[Psi]", "[", 
      RowBox[{"s", ",", "sc", ",", "vo", ",", "d", ",", "\[Nu]"}], "]"}]]}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"vOU", "[", 
   RowBox[{
   "s_", ",", "sc_", ",", "vo_", ",", "d_", ",", "\[Nu]_", ",", " ", 
    "\[Sigma]_", ",", " ", "\[Mu]_"}], "]"}], ":=", 
  RowBox[{"NIntegrate", "[", 
   RowBox[{
    FractionBox[
     RowBox[{"2", " ", "\[Mu]", " ", 
      RowBox[{"\[Sqrt]", 
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["\[Sigma]", "2"], "-", 
         SuperscriptBox["t", "2"]}], ")"}]}]}], 
     RowBox[{"\[Psi]", "[", 
      RowBox[{
       RowBox[{"s", "-", "t"}], ",", "sc", ",", "vo", ",", "d", ",", 
       "\[Nu]"}], "]"}]], ",", 
    RowBox[{"{", 
     RowBox[{"t", ",", "0", ",", "\[Sigma]"}], "}"}]}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"vFull", "[", 
   RowBox[{
   "sSt_", ",", " ", "sc_", ",", "vo_", ",", "d_", ",", "\[Nu]_", ",", " ", 
    "\[Sigma]_", ",", " ", "\[Mu]_", ",", "\[Phi]st_", ",", "ko_"}], "]"}], 
  " ", ":=", " ", 
  RowBox[{
   RowBox[{"vint", "[", 
    RowBox[{"\[Phi]st", ",", "ko", ",", " ", "0", ",", " ", "sc"}], "]"}], 
   "+", 
   RowBox[{"vOU", "[", 
    RowBox[{
    "sSt", ",", "sc", ",", "vo", ",", "d", ",", "\[Nu]", ",", " ", "\[Sigma]",
      ",", " ", "\[Mu]"}], "]"}]}]}]}], "Input",
